public class Main {
  //ascii 0 - 9
  //48 - 57 decimal
    public static Boolean detectInt(String s){
        for(int i = 0; i < s.length(); i++){
          int dec = s.charAt(i);
          if (dec >= 48 && dec <= 57){
            return true;
          }
        }
        return false;
    }
  
  public static void main(String args[]) {
      String s = "45222";
      System.out.println(detectInt(s));
    }
}